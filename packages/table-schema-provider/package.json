    "test": "NPM_ENV=test jest"
  },
  "peerDependencies": {
    "graphql": "^14.3.1",
    "react": "^16.3.0",
    "react-apollo": "^2.5.0",
    "react-dom": "^16.3.0"
  },
  "dependencies": {
    "@8base/utils": "^0.31.2",
