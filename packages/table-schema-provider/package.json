  },
  "devDependencies": {
    "@types/graphql": "^14.2.0",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.4",
    "documentation": "^8.0.0",
    "graphql": "^14.3.1",
    "jest": "24",
    "react": "^16.8.6",
    "react-apollo": "2.5.2",
