[ignore]
<PROJECT_ROOT>/packages/local-*
<PROJECT_ROOT>/.*/node_modules/graphql/.*js.flow
<PROJECT_ROOT>/packages/app-provider
<PROJECT_ROOT>/packages/utils
<PROJECT_ROOT>/packages/web-auth0-auth-client

[include]

[libs]
flow-typed/.*
flow-typed/global.js

[lints]

[options]
sharedmemory.hash_table_pow=21
module.file_ext=.js
module.file_ext=.ejs

suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe
suppress_comment= \\(.\\|\n\\)*\\$FlowIgnore

[untyped]
<PROJECT_ROOT>/.*/node_modules/react-native/.*

[strict]
